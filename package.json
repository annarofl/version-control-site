{
  "name": "TODO",
  "description": "TODO",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev && postcss static/tailwind.css -o static/index.css -w",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/index.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "format": "prettier --write ./**/*.{js,svelte,md,html,css,json}",
    "prelint": "npm run format",
    "lint": "eslint './**/*.{js,svelte}'",
    "lint:fix": "eslint --fix './**/*.{js,svelte}'",
    "test": "jest src",
    "test:watch": "npm run test --- --watch"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-svelte3": "^3.0.0",
    "jest": "^26.6.3",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.3.0",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/setupTests"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/src/__tests__/setupTests.ts"
    ]
  }
}
